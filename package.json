{
  "name": "universal-app-framework",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "clean": "turbo run clean",
    "check-env": "tsx execution/check-environment.ts",
    "sync-rules": "tsx execution/sync-antigravity-rules.ts",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prepare": "husky",
    "setup": "pnpm install && pnpm run intelligence:setup",
    "intelligence:setup": "tsx execution/setup-intelligence.ts",
    "intelligence:heal": "tsx execution/intelligence/self-healing.ts",
    "intelligence:patterns": "tsx execution/intelligence/analyze-patterns.ts",
    "intelligence:decisions": "tsx execution/intelligence/decision-engine.ts",
    "intelligence:risks": "tsx execution/intelligence/risk-assessment.ts",
    "intelligence:errors": "tsx execution/intelligence/error-mining.ts",
    "intelligence:predict": "tsx execution/intelligence/predictive-cores.ts",
    "intelligence:meta": "tsx execution/intelligence/meta-learner.ts",
    "intelligence:report": "tsx execution/intelligence/generate-report.ts",
    "intelligence:all": "pnpm run intelligence:patterns && pnpm run intelligence:errors && pnpm run intelligence:predict && pnpm run intelligence:meta && pnpm run intelligence:report",
    "monitor": "pnpm run intelligence:heal",
    "analyze": "pnpm run intelligence:all"
  },
  "devDependencies": {
    "@modelcontextprotocol/sdk": "^1.25.2",
    "@types/better-sqlite3": "^7.6.13",
    "@types/inquirer": "^9.0.9",
    "@types/node": "^20.11.0",
    "better-sqlite3": "^12.6.0",
    "husky": "^9.1.7",
    "inquirer": "^13.2.0",
    "prettier": "^3.0.0",
    "tsx": "^4.7.0",
    "turbo": "^1.10.0",
    "typescript": "^5.3.0",
    "js-yaml": "^4.1.0",
    "@types/js-yaml": "^4.0.9"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "better-sqlite3"
    ]
  }
}