{
    "mcpServers": {
        "framework": {
            "command": "tsx",
            "args": [
                ".mcp/servers/framework-server.ts"
            ]
        },
        "coordination": {
            "command": "bun",
            "args": [
                ".mcp/servers/coordination-server.ts"
            ]
        },
        "github": {
            "command": "npx",
            "args": [
                "-y",
                "@modelcontextprotocol/server-github"
            ]
        },
        "filesystem": {
            "command": "npx",
            "args": [
                "-y",
                "@modelcontextprotocol/server-filesystem",
                "."
            ]
        }
    }
}